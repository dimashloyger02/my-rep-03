{
  "version": 2,
  "builds": [
    {
      "src": "api/telegram/bot.js",
      "use": "@vercel/node"
    },
    {
      "src": "ws-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/telegram/(.*)",
      "dest": "api/telegram/bot.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/sendMessage",
      "dest": "api/telegram/sendMessage.js"
    },
    {
      "src": "/ws",
      "dest": "ws-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
